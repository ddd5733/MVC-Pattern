package packAB;

import packC.C;

public class A {
	public static void main(String[] args) {
		C cc = new C();
		String name = cc.getName();
		System.out.println("name"+name);
		
		int i =0;
		int j =1;
		int k =1;
		
		do{
			i++;
			j+=i;
			k+=j;
			
		}while(i==1);
		 System.out.println("k"+k);
		 
		}
		
	}

