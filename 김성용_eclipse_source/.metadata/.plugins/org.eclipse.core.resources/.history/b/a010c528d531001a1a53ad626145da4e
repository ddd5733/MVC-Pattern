const youtubeForm = document.querySelector(".js-youtubeForm"),
	youtubeLi = document.querySelector(".youTubeLi");


const CLICKDIV_CLASS = "div";


function MakeYoubue() {
	const youbuteDiv =youtubeLi.classList.toggle(CLICKDIV_CLASS);
	youbuteDiv.classList.add("bgImage");
}

function init() {
	youtubeLi.addEventListener("click", MakeYoubue);
}
init();